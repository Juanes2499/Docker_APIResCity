#AMBIENTE DESA

Vagrant.configure("2") do |config|
  config.vm.define :servidorUbuntuDocker do |servidorUbuntuDocker|
    servidorUbuntuDocker.vm.box = "bento/ubuntu-20.04"
    servidorUbuntuDocker.vm.network :private_network, ip: "192.170.1.2"
    servidorUbuntuDocker.vm.hostname = "servidorUbuntuDocker"
    #servidorUbuntuDocker.vm.provision "shell", path:"StartUpServer.sh"
  end
end
